<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">

            <LinearLayout
                android:id="@+id/layout_menu2"
                android:layout_width="1200dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="30dp"
                android:gravity="center"
                android:orientation="horizontal"
                tools:ignore="MissingConstraints">

                <HorizontalScrollView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:text="모델"></TextView>

                            <TextView
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="10dp"
                                android:text="카메라"></TextView>

                            <TextView
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="10dp"
                                android:text="해상도"></TextView>

                            <TextView
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="10dp"
                                android:text="채널"></TextView>

                            <TextView
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="10dp"
                                android:text="등록유형"></TextView>

                            <TextView
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="10dp"
                                android:text="주문유형"></TextView>

                            <TextView
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="10dp"
                                android:text="보드종류"></TextView>

                            <TextView
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="10dp"
                                android:text="보드타입"></TextView>


                            <TextView
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="10dp"
                                android:text="보드버전"></TextView>


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:orientation="horizontal">

                            <Spinner
                                android:id="@+id/temp1_spinner_form_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp1" />

                            <Spinner
                                android:id="@+id/temp2_spinner_form_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp2" />

                            <Spinner
                                android:id="@+id/temp3_spinner_form_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp3" />

                            <Spinner
                                android:id="@+id/temp4_spinner_form_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp4" />

                            <Spinner
                                android:id="@+id/temp5_spinner_form_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp5" />

                            <Spinner
                                android:id="@+id/temp6_spinner_form_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp6" />

                            <Spinner
                                android:id="@+id/temp7_spinner_form_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp7" />

                            <Spinner
                                android:id="@+id/temp8_spinner_form1_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp8" />

                            <Spinner
                                android:id="@+id/temp8_spinner_form2_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp8"
                                android:visibility="gone" />

                            <Spinner
                                android:id="@+id/temp8_spinner_form3_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp8"
                                android:visibility="gone" />

                            <Spinner
                                android:id="@+id/temp8_spinner_form4_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp8"
                                android:visibility="gone" />

                            <Spinner
                                android:id="@+id/temp8_spinner_form5_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp8"
                                android:visibility="gone" />

                            <Spinner
                                android:id="@+id/temp8_spinner_form6_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp8"
                                android:visibility="gone" />

                            <Spinner
                                android:id="@+id/temp8_spinner_form7_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp8"
                                android:visibility="gone" />

                            <Spinner
                                android:id="@+id/temp8_spinner_form8_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp8"
                                android:visibility="gone" />

                            <EditText
                                android:id="@+id/edt_board_version_dialog"
                                android:layout_width="120dp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/text_border"
                                android:paddingLeft="5dp"
                                android:text="" />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:text="보드코드"></TextView>

                            <TextView
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="10dp"
                                android:text="칩셋"></TextView>

                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="10dp"
                                android:text="칩셋 부품명"></TextView>

                            <TextView
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="10dp"
                                android:text="칩셋"></TextView>

                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="10dp"
                                android:text="칩셋 부품명"></TextView>

                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:layout_marginLeft="10dp"
                                android:text="배포 파일명"></TextView>


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:orientation="horizontal">


                            <EditText
                                android:id="@+id/edt_board_code_form_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:background="@drawable/text_border"
                                android:paddingLeft="5dp"
                                android:text="" />

                            <Spinner
                                android:id="@+id/temp9_spinner_form_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp3" />

                            <EditText
                                android:id="@+id/edt_chip_name1_dialog"
                                android:layout_width="250dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/text_border"
                                android:paddingLeft="5dp"
                                android:text="" />

                            <Spinner
                                android:id="@+id/temp10_spinner_form_dialog"
                                android:layout_width="120dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/spinner_background"
                                android:entries="@array/temp5" />

                            <EditText
                                android:id="@+id/edt_chip_name2_dialog"
                                android:layout_width="250dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/text_border"
                                android:paddingLeft="5dp"
                                android:text="" />

                            <EditText
                                android:id="@+id/edt_file_name_dialog"
                                android:layout_width="250dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/text_border"
                                android:paddingLeft="5dp"
                                android:text="" />


                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <Button
                                android:id="@+id/btn_before_version_dialog"
                                android:layout_width="120dp"
                                android:layout_height="45dp"
                                android:layout_marginTop="20dp"
                                android:background="@drawable/btn_background"
                                android:paddingRight="5dp"
                                android:text="이전버전"
                                android:textColor="@android:color/white" />

                            <Button
                                android:id="@+id/btn_after_version_dialog"
                                android:layout_width="120dp"
                                android:layout_height="45dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="20dp"
                                android:background="@drawable/btn_background"
                                android:paddingRight="5dp"
                                android:text="변경버전"
                                android:textColor="@android:color/white" />

                            <Button
                                android:id="@+id/btn_calendar_select_dialog"
                                android:layout_width="120dp"
                                android:layout_height="45dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="20dp"
                                android:background="@drawable/btn_background"
                                android:drawableRight="@drawable/icon_chnld"
                                android:paddingRight="5dp"
                                android:text="날짜선택"
                                android:textColor="@android:color/white" />

                            <RelativeLayout
                                android:layout_width="250dp"
                                android:layout_height="53dp"
                                android:layout_marginLeft="10dp">

                                <EditText
                                    android:id="@+id/edt_User_dialog"
                                    android:layout_width="250dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="8dp"
                                    android:background="@drawable/text_border"
                                    android:paddingLeft="5dp"
                                    android:text="" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginRight="10dp"
                                    android:background="#F0F0F0"
                                    android:text="  작성자 입력  " />

                            </RelativeLayout>


                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:text="세트"></TextView>

                                <Spinner
                                    android:id="@+id/temp11_spinner_form_dialog"
                                    android:layout_width="120dp"
                                    android:layout_height="44dp"
                                    android:layout_marginLeft="10dp"
                                    android:background="@drawable/spinner_background" />
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:orientation="horizontal">

                            <RelativeLayout
                                android:layout_width="900dp"
                                android:layout_height="53dp">

                                <EditText
                                    android:id="@+id/edt_Contents_dialog"
                                    android:layout_width="900dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="8dp"
                                    android:background="@drawable/text_border"
                                    android:paddingLeft="5dp"
                                    android:text="" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginRight="10dp"
                                    android:background="#F0F0F0"
                                    android:text="  변경사항 입력  " />
                            </RelativeLayout>

                            <Button
                                android:id="@+id/btn_item_list_update_dialog"
                                android:layout_width="120dp"
                                android:layout_height="45dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="20dp"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/btn_background"
                                android:text="수정"
                                android:textColor="@android:color/white" />

                            <Button
                                android:id="@+id/btn_item_list_cancel_dialog"
                                android:layout_width="120dp"
                                android:layout_height="45dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="20dp"
                                android:layout_marginBottom="10dp"
                                android:background="@drawable/btn_background"
                                android:text="취소"
                                android:textColor="@android:color/white" />
                        </LinearLayout>
                    </LinearLayout>
                </HorizontalScrollView>
            </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>